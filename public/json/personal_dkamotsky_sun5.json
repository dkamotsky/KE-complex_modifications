    "rules": [


	{
	    "description": "Sun Type-5 Map Again to Tilde",
	    "manipulators": [
		{
		    "type": "basic",
		    "from": { "key_code": "again",
			      "modifiers": {
				  "optional": ["any"]
			      }			    
			    },
		    "to": [
			{
			    "key_code": "grave_accent_and_tilde"
			}	    
		    ],
		    "conditions": [
			{
			    "type": "frontmost_application_unless",
			    "bundle_identifiers": ["^.*emacs.*$"]
			},
			{
			    "type": "device_if",
			    "identifiers": [
				{
				    "vendor_id": 2539,
				    "product_id": 305
				}
			    ]
			}		      
		    ]		  
		}
	    ]
	},


