{
  "title": "Personal dkamotsky Remapping for Sun Type-5",
  "rules": [


      {
	  "description": "Map Copy to Command-C",
	  "manipulators": [
	      {
		  "type": "basic",
		  "from": { "key_code": "copy" },
		  "to": [
		      {
			  "key_code": "c",
			  "modifiers": [ "left_command" ]
		      }	    
		  ],
		  "conditions": [
		      {
			  "type": "frontmost_application_unless",
			  "bundle_identifiers": ["^.*emacs.*$"]
		      }
		  ]		  
	      }
	  ]
      },

      
      {
	  "description": "Map Paste to Command-V",
	  "manipulators": [
	      {
		  "type": "basic",
		  "from": { "key_code": "paste" },
		  "to": [
		      {
			  "key_code": "v",
			  "modifiers": [ "left_command" ]
		      }	    
		  ],
		  "conditions": [
		      {
			  "type": "frontmost_application_unless",
			  "bundle_identifiers": ["^.*emacs.*$"]
		      }
		  ]		  		  
	      }
	  ]
      },
      

      {
	  "description": "Map Cut to Command-X",
	  "manipulators": [
	      {
		  "type": "basic",
		  "from": { "key_code": "cut" },
		  "to": [
		      {
			  "key_code": "x",
			  "modifiers": [ "left_command" ]
		      }	    
		  ],
		  "conditions": [
		      {
			  "type": "frontmost_application_unless",
			  "bundle_identifiers": ["^.*emacs.*$"]
		      }
		  ]		  		  
	      }
	  ]
      },

      
      {
	  "description": "Map Undo to Command-Z",
	  "manipulators": [
	      {
		  "type": "basic",
		  "from": { "key_code": "undo" },
		  "to": [
		      {
			  "key_code": "z",
